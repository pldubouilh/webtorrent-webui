{
  "name": "webtorrent-webui",
  "version": "0.0.11",
  "description": "A transmission based web interface for webtorrent",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js -v -p 9998",
    "watch": "node_modules/nodemon/bin/nodemon.js -i torrent_folder/state.json --exec 'npm run dev'",
    "debug": "node --inspect-brk index.js -v -p 9998",
    "watch-debug": "node_modules/nodemon/bin/nodemon.js -i torrent_folder/state.json --exec 'npm run debug'",
    "lint": "standard index.js handlerWebtorrent.js parser.js translator.js types.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pldubouilh/webtorrent-webui.git"
  },
  "keywords": [
    "webtorrent",
    "bittorrent",
    "remote",
    "torrent"
  ],
  "author": "pldubouilh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pldubouilh/webtorrent-webui/issues"
  },
  "bin": {
    "webtorrent-webui": "./index.js"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/pldubouilh/webtorrent-webui#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "ecstatic": "^3.0.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "parse-torrent": "^5.8.3",
    "restify": "^5.0.1",
    "rimraf": "^2.6.1",
    "webtorrent": "^0.98.19",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "standard": "^10.0.3"
  }
}
